{
  "version": 2,
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/favicon.svg",
      "dest": "/favicon.svg"
    },
    {
      "src": "/icon-192.png",
      "dest": "/icon-192.png"
    },
    {
      "src": "/icon-512.png",
      "dest": "/icon-512.png"
    },
    {
      "src": "/site.webmanifest",
      "dest": "/site.webmanifest"
    },
    {
      "src": "/scms/(.*)",
      "dest": "/scms/$1"
    },
    {
      "src": "/pdf.worker.js",
      "dest": "/pdf.worker.js"
    },
    {
      "src": "/grading-success-female.mp3",
      "dest": "/grading-success-female.mp3"
    },
    {
      "src": "/grading-success-male.mp3",
      "dest": "/grading-success-male.mp3"
    },
    {
      "src": "/notification.mp3",
      "dest": "/notification.mp3"
    },
    {
      "src": "/error.mp3",
      "dest": "/error.mp3"
    },
    {
      "src": "/sounds/(.*)",
      "dest": "/sounds/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/test-audio.html",
      "dest": "/test-audio.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "CI": "false"
  }
}